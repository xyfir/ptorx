// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts"},
        "**/.vs": true,
        "**/.vscode": true,
        "**/node_modules": true,
        "**/*.njsproj": true,
        "**/typings": true,
        "**/*.sln": true,
        "**/*.dat": true,
        "**/*.map": true,
        "**/bin": true,
        "**/obj": true
    }
}